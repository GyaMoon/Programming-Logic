Proceso Ejercicio_8
	Definir a, f, m, pf, pm Como Real;
	Escribir "Proporcione la cantidad de hombres";
	Leer m;
	Escribir "Proporcione la cantidad de mujeres";
	Leer f;
	
	a <- f + m;
	pf <- f * 100 / a;
	pm <- m * 100 / a;

	Escribir "Porcentaje de hombres: %", pm;
	Escribir "Porcentaje de mujeres: %", pf;

	
	
FinProceso
