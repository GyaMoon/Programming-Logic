Proceso Ejercicio_9
	Definir n1, n2, s, r, m, d Como Real;
	Escribir "Numero uno: ";
	Leer n1;
	Escribir "Numero dos; ";
	Leer n2;
	
	s <- n1 + n2;
	r <- n1 - n2;
	m <- n1 * n2;
	d <- n1 / n2;
	
	Escribir "Resultado suma: ", s;
	Escribir "Resultado resta; ", r;
	Escribir "Resultado multiplicación: ", m;
	Escribir "Resultado división: ", d;

FinProceso
